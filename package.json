{
  "name": "cnidaria-admin-curve",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --host --open",
    "dev:no-open": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --open",
    "clean": "rm -rf node_modules/.vite .vite dist .cache",
    "clean:all": "rm -rf node_modules package-lock.json && npm install",
    "deploy:dev": "./deploy-cloudrun-dev.sh",
    "deploy:stage": "./deploy-cloudrun-stage.sh",
    "deploy:prod": "./deploy-cloudrun-prod.sh",
    "deploy": "npm run deploy:dev",
    "cloud-run:status": "gcloud run services list --region=us-central1",
    "cloud-run:logs:dev": "gcloud run services logs read cnidaria-admin-curves-dev --region=us-central1",
    "cloud-run:logs:stage": "gcloud run services logs read cnidaria-admin-curves-stage --region=us-central1",
    "cloud-run:logs:prod": "gcloud run services logs read cnidaria-admin-curves-prod --region=us-central1"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/three": "0.162.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "three": "0.179.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
